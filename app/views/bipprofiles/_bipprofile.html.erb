<div class="modal">
<table>
	<tr>
		<th>Profile Name</th>
		<th>Profile Type</th>
		<th>Migrated</th>
	</tr>
	<tr>
		<td><%= @bipprofile.name %></td>
		<td><%= @bipprofile.type %></td>
		<td>
			<div class="migrated-image">
				<% if @bipprofile.migrated %>
					<%= image_tag "green-check.png" %>
				<% else %>
					<%= image_tag "red-x.png" %>
				<% end %>
			</div>
		</td>
	</tr>
</table>
</div>