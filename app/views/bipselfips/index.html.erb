<h3>Bipselfips</h3>

<table class="zebra-striped">
	<thead>
		<tr>
			<th>SelfIp</th>
			<th>Netmask</th>
			<th>Unit</th>
			<th>Floating</th>
			<th>VLAN</th>
		</tr>
	</thead>
	<tbody>
		<% @bipselfip.each do |bip_selfip| %>
			<tr>
			<td><%= bip_selfip.name %></td>
			<td><%= bip_selfip.netmask %></td>
			<td><%= bip_selfip.unit %></td>
			<td><%= bip_selfip.floating %></td>
			<td><%= bip_selfip.vlan %></td>
			</tr>
		<% end %>
	</tbody>
</table>

