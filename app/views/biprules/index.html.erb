<p id="notice"><%= notice %></p>

<h3>Biprules</h3>

<b>Name:</b>
<%= @bip_config.name %>

<table>
	<tr>
		<th>Migrated</th>
		<th>Name</th>
		<th>Content</th>
	</tr>
	<% @biprules.each do |bip_rule| %>
		<tr>
			<td><%= render :partial => "migrated", :locals => { :bip_rule => bip_rule } %></td>
			<td><%= bip_rule.name %></td>
			<td><%= render :partial => "content", :locals => { :bip_rule => bip_rule } %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Back', bip_config_path(@bip_config) %>