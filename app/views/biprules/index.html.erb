<p id="notice"><%= notice %></p>

<h3>Biprules</h3>

<b>Name:</b>
<%= @bip_config.name %>

<table>
	<tr>
		<th>Name</th>
		<th>Content</th>
	</tr>
	<% @biprules.each do |bip_rule| %>
		<tr>
		<td><%= bip_rule.name %></td>
		<td><pre>
		<% bip_rule.content.each do |m| %>
			<%= m %><br />
		<% end %>
		</pre></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Back', bip_config_path(@bip_config) %>