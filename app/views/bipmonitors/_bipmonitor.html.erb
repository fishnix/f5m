<table>
	<thead>
		<tr>
			<th>Content</th>
			<th>Migrated</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><pre><%= @bipmonitor.content %></pre></td>
			<td>
				<div class="migrated-image">
					<% if @bipmonitor.migrated %>
						<%= image_tag "green-check.png", :width => 50, :height => 50 %>
					<% else %>
						<%= image_tag "red-x.png", :width => 50, :height => 50  %>
					<% end %>
				</div>
			</td>
		</tr>
	</tbody>
</table>
