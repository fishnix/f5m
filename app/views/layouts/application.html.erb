<!DOCTYPE html>
<html>
<head>
  <title>F5m</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="headernav">
	<% if @bip_config.nil? %>
		<div id='topheader'>
			<%= render :partial => 'bip_configs/homeheader' %>
		</div>
		<nav>
			<%= render :partial => 'bip_configs/homemenu' %>
		</nav>
	<% else %>
		<div id='topheader'>
			<%= render :partial => 'bip_configs/topheader', :locals => { :bip_config => @bipconfig } %>
		</div>
		<nav>
			<%= render :partial => 'bip_configs/topmenu', :locals => { :bip_config => @bipconfig } %>
		</nav>
	<% end %>
</div>

<div id='content'>
	<%= yield %>
<div>
	
<footer>
	<%= render :partial => "layouts/footer" %>
</footer>

</body>
</html>
